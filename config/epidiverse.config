// epidiverse.config

workDir = '/scr/epi/pipelines/.wgbs'
singularity.cacheDir = '/scr/epi/.containers'

params {

	thlaspi = false
	fragaria = false
	populus = false

}

executor {
	
	$slurm {
		jobName = { "${task.name}".replaceAll("\\s","") }
		submitRateLimit = '20 min'
		queueSize = 20
		pollInterval = '30 sec'
	}
}